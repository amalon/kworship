<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-kworship+xml">
    <sub-class-of type="application/xml"/>
    <comment>KWorship data file</comment>
    <glob pattern="*.kw"/>
  </mime-type>
  <mime-type type="application/x-kworship-archive">
    <sub-class-of type="application/x-bzip2"/>
    <comment>KWorship data archive</comment>
    <glob pattern="*.kwz"/>
  </mime-type>
</mime-info>
